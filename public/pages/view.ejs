<div class="jumbotron text-center">
	<h1>All added Videos</h1>

    <a href="#admin" class="btn btn-default" role="button">Admin Mode</a>

	<p>All Added Videos!</p>
</div>


  <div ng-controller="viewController as list">
	<table align="center" valign="middle">
        <tr>
            <th><span  style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign="middle";>Title</span></th>
            <th><span  style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign="middle";>User<span style=margin-left:5px;" class="glyphicon glyphicon-user"></span></span></th>
            <th><span  style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign="middle";>Veto</span></th>
        </tr>
	  <tbody ng-repeat="video in videos | orderBy:'video.id'">

        <tr style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign="middle"; >
            <td>
                <span  style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign="middle";>{{video.vidName}}</span>
            </td>
            <td>
                <span style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign="middle";>{{video.user}}</span>
            </td>
             <td>
                 <span style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign="middle";>{{video.veto}}<span style=margin-left:10px;" class="glyphicon glyphicon-thumbs-down" ng-click="incrementVeto(video._id)"></span></span>
            </td>

    	</tr>
      </tbody>
    </table> 
  </div>
